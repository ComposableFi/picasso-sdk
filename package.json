{
  "name": "picasso-sdk",
  "version": "0.3.187",
  "description": "Picasso sdk for cosmos, ethereum, solana, and polkadot ibc transfer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "picasso",
    "composable",
    "cosmos",
    "ethereum",
    "solana",
    "polkadot"
  ],
  "author": "composable finance",
  "license": "ISC",
  "scripts": {
    "build": "tsc && webpack --mode production && npm run test",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "publish-npm": "npm version patch && rm -rf ./dist && npm run build && npm publish --access public",
    "generate-helper": "ts-node ./src/scripts/generate-helper.ts",
    "generate-multihop": "ts-node ./src/scripts/generate-multihop.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@cosmjs/stargate": "^0.32.4",
    "@cosmjs/tendermint-rpc": "^0.32.4",
    "@injectivelabs/sdk-ts": "1.14.4",
    "@polkadot/api": "^11.3.1",
    "@polkadot/extension-dapp": "^0.46.3",
    "@polkadot/extension-inject": "^0.46.3",
    "@polkadot/keyring": "12.2.1",
    "@polkadot/util": "^12.2.1",
    "@polkadot/util-crypto": "^12.2.1",
    "@solana/spl-token": "^0.3.10",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.3.4",
    "bech32": "^2.0.0",
    "big.js": "^6.2.1",
    "borsher": "^3.5.0",
    "eventemitter3": "^5.0.0",
    "js-sha256": "^0.11.0",
    "secretjs": "1.9.3",
    "web3": "^1.6.0",
    "web3-core": "1.10.2",
    "web3-eth-contract": "1.6.1",
    "web3-types": "^1.2.0",
    "web3-utils": "^1.6.0"
  },
  "files": [
    "dist/chains/**/*",
    "dist/config/**/*",
    "dist/indexer/**/*",
    "dist/index.*"
  ],
  "devDependencies": {
    "@keplr-wallet/types": "^0.11.26",
    "@polkadot/api-base": "^11.3.1",
    "@polkadot/rpc-core": "^11.3.1",
    "@polkadot/types": "^11.3.1",
    "@polkadot/types-codec": "^11.3.1",
    "@types/jest": "^29.5.12",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "type-fest": "^4.23.0",
    "typescript": "^5.5.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  }
}
